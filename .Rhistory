	best <- sum(delta[,1] == 0) / length(delta[,1]);#
	best <- c(best, 1-best);#
	#
	modelSummary <- cbind(Model=c("Base", "Sisters"), Prop.best=best, res);#
	#
	return(modelSummary);#
